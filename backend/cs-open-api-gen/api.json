{
    "openapi": "3.0.0",
    "info": {
    "title": "Conversion service API",
        "version": "5.9.4"
    },
    "paths": {
        "/jobs": {
            "post": {
                "operationId": "createJob",
                "summary": "Create one conversion job",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "required": [
                                    "recipe",
                                    "input"
                                ],
                                "properties": {
                                    "recipe": {
                                        "description": "Name of a conversion recipe",
                                        "type": "string",
                                        "example": "fbx2wmd",
                                        "enum": [
                                            "fbx2wmd",
                                            "fbx2wmdOpt",
                                            "ifc2wmd",
                                            "ifc2wmdOpt"
                                        ]
                                    },
                                    "input": {
                                        "description": "Input file",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "callback": {
                                        "description": "Webhook called after finishing a job",
                                        "type": "string"
                                    }
                                }
                            }
                        },
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "recipe": {
                                        "description": "Name of a conversion recipe",
                                        "type": "string",
                                        "example": "fbx2wmd",
                                        "enum": [
                                            "fbx2wmd",
                                            "fbx2wmdOpt",
                                            "ifc2wmd",
                                            "ifc2wmdOpt"
                                        ]
                                    },
                                    "input": {
                                        "description": "Input file",
                                        "type": "string",
                                        "example": "https://github.com/KhronosGroup/glTF-Sample-Models/raw/master/sourceModels/Avocado/Avocado.fbx"
                                    },
                                    "callback": {
                                        "description": "Webhook called after finishing a job",
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "200 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "description": "ID of the created job",
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/jobs/{id}/status": {
            "get": {
                "operationId": "getJobStatus",
                "summary": "Get a conversion job status",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID of a conversion job",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "200 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "description": "Conversion job status",
                                            "enum": [
                                                "pending",
                                                "working",
                                                "finished",
                                                "error",
                                                "canceled"
                                            ]
                                        },
                                        "progress": {
                                            "type": "number",
                                            "minimum": 0,
                                            "maximum": 1
                                        }
                                    }
                                },
                                "examples": {
                                    "pending": {
                                        "value": {
                                            "status": "pending",
                                            "progress": 0
                                        }
                                    },
                                    "working": {
                                        "value": {
                                            "status": "working",
                                            "progress": 0.87
                                        }
                                    },
                                    "finished": {
                                        "value": {
                                            "status": "finished",
                                            "progress": 1
                                        }
                                    },
                                    "error": {
                                        "value": {
                                            "status": "error",
                                            "progress": 0.66
                                        }
                                    },
                                    "canceled": {
                                        "value": {
                                            "status": "canceled",
                                            "progress": 0.66
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "404 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "example": "job with {id} was not found"
                                }
                            }
                        }
                    }
                }
            }
        },
