{
  "name": "backned-sandbox-server",
  "version": "0.0.1",
  "description": "Simple server that returns env var for each request",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "seed": "knex seed:run --knexfile db/knexfile.js",
    "setup": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "better-sse": "^0.10.0",
    "cs-open-api": "file:cs-open-api-gen/cs-open-api-2.1.5.tgz",
    "ejs": "^3.1.9",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "koa-views": "^8.1.0",
    "minio": "^7.1.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
